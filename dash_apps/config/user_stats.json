{
  "ui": {
    "template_style": {
      "height": "400px",
      "width": "100%",
      "card_height": "350px",
      "card_width": "100%",
      "min_height": "300px"
    }
  },
  "cache": {
    "ttl": 300,
    "key_prefix": "user_stats",
    "enable_cache": true
  },
  "calculations": {
    "stats_fields": [
      "passenger_trips",
      "driver_trips", 
      "total_trips",
      "total_distance",
      "driver_distance",
      "passenger_distance"
    ],
    "default_values": {
      "passenger_trips": 0,
      "driver_trips": 0,
      "total_trips": 0,
      "total_distance": 0.0,
      "driver_distance": 0.0,
      "passenger_distance": 0.0,
      "db_error": false
    }
  }
}
