{
  "profile": {
    "panel_name": "profile",
    "description": "Panneau du profil utilisateur",
    "methods": {
      "cache": {
        "html_cache_enabled": true,
        "redis_cache_enabled": false,
        "cache_ttl": 600
      },
      "data_fetcher": {
        "type": "rest",
        "inputs": {
          "user_id": "required"
        },
        "rest_config": {
          "function": "get_user_profile",
          "module": "dash_apps.utils.data_schema_rest",
          "params": {
            "user_id": "user_id"
          }
        }
      },
      "renderer": {
        "type": "function",
        "inputs": {
          "user_id": "required",
          "data": "required"
        },
        "module": "dash_apps.components.user_profile",
        "function": "render_user_profile"
      }
    }
  },
  "stats": {
    "panel_name": "stats",
    "description": "Panneau des statistiques utilisateur",
    "methods": {
      "cache": {
        "html_cache_enabled": true,
        "redis_cache_enabled": false,
        "cache_ttl": 600
      },
      "data_fetcher": {
        "type": "rest",
        "inputs": {
          "user_id": "required"
        },
        "rest_config": {
          "function": "get_user_stats_optimized",
          "module": "dash_apps.utils.data_schema_rest",
          "params": {
            "user_id": "user_id"
          }
        }
      },
      "renderer": {
        "type": "function",
        "inputs": {
          "user_id": "required",
          "data": "required"
        },
        "module": "dash_apps.components.user_stats",
        "function": "render_user_stats"
      }
    }
  },
  "trips": {
    "panel_name": "trips",
    "description": "Panneau des trajets utilisateur",
    "methods": {
      "cache": {
        "html_cache_enabled": true,
        "redis_cache_enabled": false,
        "cache_ttl": 600
      },
      "data_fetcher": {
        "type": "rest",
        "inputs": {
          "user_id": "required"
        },
        "rest_config": {
          "function": "get_user_trips_with_role",
          "module": "dash_apps.utils.data_schema_rest",
          "params": {
            "user_id": "user_id",
            "limit": 50
          }
        }
      },
      "renderer": {
        "type": "function",
        "inputs": {
          "user_id": "required",
          "data": "required"
        },
        "module": "dash_apps.components.user_trips",
        "function": "render_user_trips"
      }
    }
  }
}
